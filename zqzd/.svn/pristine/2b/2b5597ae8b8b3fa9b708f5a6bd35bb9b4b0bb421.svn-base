<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.dao.CouponWalletInfoLogDao">

    <resultMap type="com.platform.entity.CouponWalletInfoLogEntity" id="CouponwalletInfoLogMap">
        <result property="id" column="id"/>
        <result property="wid" column="wid"/>
        <result property="uid" column="uid"/>
        <result property="wtype1" column="wtype1"/>
        <result property="wtype2" column="wtype2"/>
        <result property="money" column="money"/>
        <result property="preMoney" column="pre_money"/>
        <result property="endMoney" column="end_money"/>
        <result property="fMoney" column="f_money"/>
        <result property="fStatus" column="f_status"/>
        <result property="title" column="title"/>
        <result property="content" column="content"/>
        <result property="ctime" column="ctime"/>
    </resultMap>

	<select id="queryObject" resultType="com.platform.entity.CouponWalletInfoLogEntity">
		select
			`id`,
			`wid`,
			`uid`,
			`wtype1`,
			`wtype2`,
			`money`,
			`pre_money`,
			`end_money`,
			`f_money`,
			`f_status`,
			`title`,
			`content`,
			`ctime`
		from coupon_wallet_info_log
		where id = #{id}
	</select>

	<select id="queryList" resultType="com.platform.entity.CouponWalletInfoLogEntity">
		select
    		`id`,
    		`wid`,
    		`uid`,
    		`wtype1`,
    		`wtype2`,
    		`money`,
    		`pre_money`,
    		`end_money`,
    		`f_money`,
    		`f_status`,
    		`title`,
    		`content`,
    		`ctime`
		from coupon_wallet_info_log
		WHERE 1=1
		<if test="name != null and name.trim() != ''">
			AND name LIKE concat('%',#{name},'%')
		</if>
        <choose>
            <when test="sidx != null and sidx.trim() != ''">
                order by ${sidx} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from coupon_wallet_info_log
		WHERE 1=1
        <if test="name != null and name.trim() != ''">
            AND name LIKE concat('%',#{name},'%')
        </if>
	</select>
	 
	<insert id="save" parameterType="com.platform.entity.CouponWalletInfoLogEntity" useGeneratedKeys="true" keyProperty="id">
		insert into coupon_wallet_info_log(
			`wid`,
			`uid`,
			`wtype1`,
			`wtype2`,
			`money`,
			`pre_money`,
			`end_money`,
			`f_money`,
			`f_status`,
			`title`,
			`content`,
			`ctime`)
		values(
			#{wid},
			#{uid},
			#{wtype1},
			#{wtype2},
			#{money},
			#{preMoney},
			#{endMoney},
			#{fMoney},
			#{fStatus},
			#{title},
			#{content},
			#{ctime})
	</insert>
	 
	<update id="update" parameterType="com.platform.entity.CouponWalletInfoLogEntity">
		update coupon_wallet_info_log 
		<set>
			<if test="wid != null">`wid` = #{wid}, </if>
			<if test="uid != null">`uid` = #{uid}, </if>
			<if test="wtype1 != null">`wtype1` = #{wtype1}, </if>
			<if test="wtype2 != null">`wtype2` = #{wtype2}, </if>
			<if test="money != null">`money` = #{money}, </if>
			<if test="preMoney != null">`pre_money` = #{preMoney}, </if>
			<if test="endMoney != null">`end_money` = #{endMoney}, </if>
			<if test="fMoney != null">`f_money` = #{fMoney}, </if>
			<if test="fStatus != null">`f_status` = #{fStatus}, </if>
			<if test="title != null">`title` = #{title}, </if>
			<if test="content != null">`content` = #{content}, </if>
			<if test="ctime != null">`ctime` = #{ctime}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from coupon_wallet_info_log where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from coupon_wallet_info_log where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>