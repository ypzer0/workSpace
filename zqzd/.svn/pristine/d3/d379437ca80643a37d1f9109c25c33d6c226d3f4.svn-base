import * as types from './mutation-types'

export const mutations = {

  [types.INIT_GOODS_DETAIL] (state, o) {
    state.GoodsInfo=o;
  },

  [types.GET_SELECT_INFO] (state, o) {
    state.doctorList=o.doctorList;
    state.departmentList=o.departmentList;
    state.hospitalList=o.hospitalList;
  }


}
