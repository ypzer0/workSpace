<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.platform.dao.ApiSysOssMapper">


	 
	<insert id="save" parameterType="com.platform.entity.SysOssVo" useGeneratedKeys="true" keyProperty="id">
		insert into sys_oss
		(
			`url`, 
			`create_date`
		)
		values
		(
			#{url}, 
			#{createDate}
		)
	</insert>
	 


</mapper>