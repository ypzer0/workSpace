  //寄卖-纠纷中
  [types.INIT_DISPUTES] (state, o) {
    //debugger;
    state.disputesSell = o.couponSell;
  },

  //寄卖-出售中-下架
  [types.INIT_SHELVES] (state, o) {
    //debugger;
    state.shelves = o;
  },

  //寄卖-出售中-上架
  [types.INIT_GOODS_SOLD_OUT] (state, o) {
    //debugger;
    state.goodsSoldOut = o;
  },
